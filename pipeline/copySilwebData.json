{
	"name": "copySilwebData",
	"properties": {
		"activities": [
			{
				"name": "LoadSilweb",
				"type": "ForEach",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@pipeline().parameters.cw_items",
						"type": "Expression"
					},
					"activities": [
						{
							"name": "MysqlFromBlob",
							"type": "Copy",
							"dependsOn": [],
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [
								{
									"name": "Source",
									"value": "@{item().source.tableName}"
								},
								{
									"name": "Destination",
									"value": "loaddatacontainer//@{item().destination.fileName}"
								}
							],
							"typeProperties": {
								"source": {
									"type": "MySqlSource"
								},
								"sink": {
									"type": "DelimitedTextSink",
									"storeSettings": {
										"type": "AzureBlobFSWriteSettings"
									},
									"formatSettings": {
										"type": "DelimitedTextWriteSettings",
										"quoteAllText": true,
										"fileExtension": ".csv"
									}
								},
								"enableStaging": false,
								"validateDataConsistency": false,
								"translator": {
									"type": "TabularTranslator",
									"mappings": [
										{
											"source": {
												"name": "id",
												"type": "Int32"
											},
											"sink": {
												"name": "id",
												"type": "String"
											}
										},
										{
											"source": {
												"name": "id_pregao",
												"type": "Int32"
											},
											"sink": {
												"name": "id_pregao",
												"type": "String"
											}
										},
										{
											"source": {
												"name": "id_fornecedor",
												"type": "Int32"
											},
											"sink": {
												"name": "id_fornecedor",
												"type": "String"
											}
										},
										{
											"source": {
												"name": "num_item",
												"type": "String"
											},
											"sink": {
												"name": "num_item",
												"type": "String"
											}
										},
										{
											"source": {
												"name": "descricao_item",
												"type": "String"
											},
											"sink": {
												"name": "descricao_item",
												"type": "String"
											}
										},
										{
											"source": {
												"name": "valor_estimado",
												"type": "Decimal"
											},
											"sink": {
												"name": "valor_estimado",
												"type": "String"
											}
										},
										{
											"source": {
												"name": "unidade_fornecimento",
												"type": "String"
											},
											"sink": {
												"name": "unidade_fornecimento",
												"type": "String"
											}
										},
										{
											"source": {
												"name": "quantidade",
												"type": "Int32"
											},
											"sink": {
												"name": "quantidade",
												"type": "String"
											}
										},
										{
											"source": {
												"name": "objeto",
												"type": "String"
											},
											"sink": {
												"name": "objeto",
												"type": "String"
											}
										},
										{
											"source": {
												"name": "situacao",
												"type": "String"
											},
											"sink": {
												"name": "situacao",
												"type": "String"
											}
										},
										{
											"source": {
												"name": "data_cadastro",
												"type": "DateTime"
											},
											"sink": {
												"name": "data_cadastro",
												"type": "String"
											}
										},
										{
											"source": {
												"name": "observacao",
												"type": "String"
											},
											"sink": {
												"name": "observacao",
												"type": "String"
											}
										},
										{
											"source": {
												"name": "status",
												"type": "Int32"
											},
											"sink": {
												"name": "status",
												"type": "String"
											}
										},
										{
											"source": {
												"name": "referencia",
												"type": "String"
											},
											"sink": {
												"name": "referencia",
												"type": "String"
											}
										},
										{
											"source": {
												"name": "uf_uasg",
												"type": "String"
											},
											"sink": {
												"name": "uf_uasg",
												"type": "String"
											}
										}
									]
								}
							},
							"inputs": [
								{
									"referenceName": "SourceDataset",
									"type": "DatasetReference",
									"parameters": {
										"cw_tableName": "@item().source.tableName"
									}
								}
							],
							"outputs": [
								{
									"referenceName": "TargetDataset",
									"type": "DatasetReference"
								}
							]
						}
					]
				}
			}
		],
		"parameters": {
			"cw_items": {
				"type": "Array",
				"defaultValue": [
					{
						"source": {
							"tableName": "`anexo_item`"
						},
						"destination": {
							"fileName": "`anexo_item`.txt"
						}
					},
					{
						"source": {
							"tableName": "`edital_pregao`"
						},
						"destination": {
							"fileName": "`edital_pregao`.txt"
						}
					},
					{
						"source": {
							"tableName": "`fornecedor`"
						},
						"destination": {
							"fileName": "`fornecedor`.txt"
						}
					},
					{
						"source": {
							"tableName": "`item`"
						},
						"destination": {
							"fileName": "`item`.txt"
						}
					},
					{
						"source": {
							"tableName": "`pregao_fornecedor_valor`"
						},
						"destination": {
							"fileName": "`pregao_fornecedor_valor`.txt"
						}
					},
					{
						"source": {
							"tableName": "`pregao`"
						},
						"destination": {
							"fileName": "`pregao`.txt"
						}
					},
					{
						"source": {
							"tableName": "`uasg`"
						},
						"destination": {
							"fileName": "`uasg`.txt"
						}
					}
				]
			}
		},
		"annotations": []
	}
}